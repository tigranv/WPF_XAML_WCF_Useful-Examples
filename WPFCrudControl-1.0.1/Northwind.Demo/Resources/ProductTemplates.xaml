<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:productsViews="clr-namespace:Northwind.Demo.Views.Products"
                    xmlns:productViewModels="clr-namespace:Northwind.Demo.ViewModel.Products">
    
    <DataTemplate DataType="{x:Type productViewModels:ProductsSearchViewModel}">
        <productsViews:ProductsSearch/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type productViewModels:ProductAddEditViewModel}">
        <productsViews:ProductAddEdit/>
    </DataTemplate>
    
</ResourceDictionary>